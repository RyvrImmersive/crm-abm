# Production Environment Variables
# Copy this file to .env.production and fill in your values

# AstraDB Configuration
ASTRA_DB_TOKEN=AstraCS:your_token_here
ASTRA_DB_ENDPOINT=https://your-db-id-region.apps.astra.datastax.com

# Langflow Configuration  
LANGFLOW_API_KEY=sk-your_langflow_key_here
LANGFLOW_FLOW_URL=https://your-langflow-instance.onrender.com/api/v1/run/your-flow-id

# External APIs for Lookalike Companies
EXA_API_KEY=your_exa_api_key_here
TAVILY_API_KEY=tvly-your_tavily_key_here

# Application Configuration
LOG_LEVEL=INFO
NODE_ENV=production

# Frontend Configuration (for React build)
REACT_APP_API_URL=https://your-backend-url.onrender.com
